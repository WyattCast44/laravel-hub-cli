{
    "name": "laravel-hub-cli/laravel-hub",
    "description": "A tool for scaffolding new Laravel applications.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/laravel-hub-cli/laravel-hub-cli",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-hub-cli/laravel-hub-cli/issues",
        "source": "https://github.com/laravel-hub-cli/laravel-hub-cli"
    },
    "authors": [
        {
            "name": "Wyatt Castaneda",
            "email": "wyatt.castaneda@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel-zero/framework": "^8.4",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["laravel-hub"]
}
